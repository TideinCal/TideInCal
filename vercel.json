{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/app",
      "dest": "/public/index.html"
    },
    {
      "src": "/",
      "dest": "/public/polishPre.html"
    },
    {
    "src": "/(.*)",
    "dest": "/server.js"
    }
  ]
}
